


{{#each char-info}}

 <section id="user-infos" class="container">
    <h1>Characters</h1>
    <div class="user-info char-info">
      <h3>{{character_name}}</h3>
      <h5>Current Campaign</h5>
      <li><a href="#" class="btn btn-reverse">Member1</a></li>
